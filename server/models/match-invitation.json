{
  "name": "MatchInvitation",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "Status": {
      "type": "number",
      "required": true,
      "default": 0
    }
  },
  "validations": [],
  "relations": {
    "Host": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "HostId"
    },
    "Invitee": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "InviteeId"
    },
    "Match": {
      "type": "belongsTo",
      "model": "Match",
      "foreignKey": "MatchId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    }
  ],
  "methods": {}
}
